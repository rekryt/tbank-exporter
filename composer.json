{
    "name": "tbank/exporter",
    "authors": [
        {
            "name": "Rekryt",
            "email": "rekrytkw@gmail.com"
        }
    ],
    "require": {
        "opencck/server": "v1.0.0",
        "amphp/http-client": "v5.2.1",
        "amphp/websocket-client": "v2.0.0",
        "vlucas/phpdotenv": "^5.5",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9",
        "amphp/phpunit-util": "^3"
    },
    "autoload": {
        "files": [
            "src/functions.php"
        ],
        "psr-4": {
            "TBank\\": [
                "src/",
                "packages/tbank/trading/src/"
            ]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TBank\\": [
                "test/",
                "packages/tbank/trading/test/"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
